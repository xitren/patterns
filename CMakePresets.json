{
    "version": 8,
    "configurePresets": [
        {
            "name": "clang",
            "displayName": "Clang 16.0.0 x86_64-w64-windows-gnu",
            "description": "Using compilers: C = /mnt/c/MinGW/mingw64/bin/clang.exe, CXX = /mnt/c/MinGW/mingw64/bin/clang++.exe",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/mnt/c/MinGW/mingw64/bin/clang.exe",
                "CMAKE_CXX_COMPILER": "/mnt/c/MinGW/mingw64/bin/clang++.exe",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "gcc12",
            "displayName": "GCC 12.3.0 x86_64-linux-gnu",
            "description": "Using compilers: C = /usr/bin/gcc-12, CXX = /usr/bin/g++-12",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/gcc-12",
                "CMAKE_CXX_COMPILER": "/usr/bin/g++-12",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}